#apply slowness effect
execute if score @s ha.armorcount matches 10..24 run effect give @s minecraft:slowness 5 0 true
execute if score @s ha.armorcount matches 25..39 run effect give @s minecraft:slowness 5 1 true
execute if score @s ha.armorcount matches 40..54 run effect give @s minecraft:slowness 5 2 true
execute if score @s ha.armorcount matches 55..69 run effect give @s minecraft:slowness 5 3 true
